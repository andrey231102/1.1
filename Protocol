using System;

namespace ConsoleApp1
{
class Program
{
static void Main(string[] args)
{
Console.WriteLine("Enter ur name");
string myName = Console.ReadLine();

Console.WriteLine("Enter ur age");
int myAge = Convert.ToInt32(Console.ReadLine());

Console.WriteLine("Enter ur zodiac sign");
string myZodiacSign = Console.ReadLine();

Console.WriteLine("Enter ur current job");
string myJob = Console.ReadLine();

Console.WriteLine($"Вас зовут {myName}, вам {myAge}, вы {myZodiacSign} и вы {myJob}.");
}
}
}
